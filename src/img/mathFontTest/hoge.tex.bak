\newcommand*\samplemath{%
  \newcommand*\suminfn{\sum^\infty_{n = 0}}%
  \newcommand*\dif{\;\mathrm{d}}%
%\begin{equation*}% p. 180 
%  \int \suminfn c_n z_n \dif{z}
%  = \suminfn c_n \int z^n \dif{z}
%  = \suminfn\ c_n \frac{1}{n + 1} z^{n + 1} + \alpha
%\end{equation*}%
\begin{eqnarray*}% p. 199
 f(z) & = & \frac{1}{2\pi i} \oint_C \suminfn \frac{(z - a)^n}{(z_0 - a)^{n
            + 1}} f(z_0) \dif{z_0} \\
      & = & \suminfn \frac{(z - a)^n}{n!} \frac{n!}{2\pi i}
            \oint_C \frac{f(z_0)}{(z_0 - a)^{n + 1}} \dif{z_0}\\
      & = & \suminfn \frac{(z-a)^n}{n!} f^{(n)}(a)
\end{eqnarray*}%
}
